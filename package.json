{
  "name": "MurrayDeskBooking",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "ios:debug:dev": "ENVFILE=.env.development react-native run-ios --mode Debug-Dev",
    "ios:debug:prod": "ENVFILE=.env.production react-native run-ios --mode Debug-Production",
    "ios:release:dev": "ENVFILE=.env.development react-native run-ios --mode Release-Dev",
    "ios:release:prod": "ENVFILE=.env.production react-native run-ios --mode Release-Production",
    "ios:qa:distribute": "bundle exec fastlane ios qa_distribute",
    "ios:setup": "bundle exec fastlane ios setup_profiles_certs",
    "android:debug:dev": "ENVFILE=.env.development react-native run-android --mode DevelopmentDebug --appIdSuffix=development",
    "android:release:dev": "ENVFILE=.env.development react-native run-android --mode DevelopmentRelease --appIdSuffix=development",
    "android:debug:prod": "ENVFILE=.env.production react-native run-android --mode ProductionDebug --appIdSuffix=production",
    "android:release:prod": "ENVFILE=.env.production react-native run-android --mode ProductionRelease --appIdSuffix=production",
    "android:qa:distribute": "bundle exec fastlane android qa_distribute",
    "lint": "eslint .",
    "test": "jest",
    "test-functions": "cd functions && yarn ci-test",
    "prepush": "jest",
    "deployFirestoreRules": "firebase deploy --only firestore:rules",
    "deployStorageRules": "firebase deploy --only storage",
    "deployFirebaseFunctions": "cd functions && yarn deploy",
    "installFirebaseFunctions": "cd functions && yarn install",
    "emulateFirebase:dev": "firebase use development; firebase emulators:start --only functions,firestore --import=EmulatorData --export-on-exit=EmulatorData",
    "emulateFirebase:prod": "firebase use production;  firebase emulators:start --only functions,firestore --import=EmulatorData --export-on-exit=EmulatorData",
    "clean-caches": "./cleancaches.sh",
    "typecheck": "tsc --noEmit",
    "typecheck-functions": "cd functions && yarn typecheck"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "7.18.9",
    "@react-native-community/cli": "11.4.1",
    "@react-native-firebase/analytics": "18.9.0",
    "@react-native-firebase/app": "18.9.0",
    "@react-native-firebase/app-check": "18.9.0",
    "@react-native-firebase/auth": "18.9.0",
    "@react-native-firebase/crashlytics": "18.9.0",
    "@react-native-firebase/firestore": "18.9.0",
    "@react-native-firebase/functions": "18.9.0",
    "@react-native-firebase/messaging": "18.9.0",
    "@react-native-firebase/remote-config": "18.9.0",
    "@react-native-google-signin/google-signin": "11.0.1",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-native-segmented-control/segmented-control": "2.5.7",
    "@react-navigation/bottom-tabs": "6.6.1",
    "@react-navigation/native": "6.1.18",
    "@react-navigation/native-stack": "6.11.0",
    "@react-navigation/stack": "6.4.1",
    "@reduxjs/toolkit": "1.9.7",
    "dayjs": "1.11.13",
    "firebase-admin": "12.7.0",
    "native-base": "3.4.28",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-bootsplash": "5.5.3",
    "react-native-config": "1.5.3",
    "react-native-device-info": "10.14.0",
    "react-native-exit-app": "2.0.0",
    "react-native-gesture-handler": "2.20.2",
    "react-native-reanimated": "3.16.7",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "3.35.0",
    "react-native-svg": "14.1.0",
    "react-native-uuid": "2.0.3",
    "react-redux": "8.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-typescript": "7.26.0",
    "@babel/runtime": "7.26.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@testing-library/jest-native": "5.4.3",
    "@testing-library/react-native": "12.8.1",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.14",
    "@types/mocha": "10.0.10",
    "@types/node": "18.19.71",
    "@types/react": "18.3.18",
    "@types/react-redux": "7.1.34",
    "@types/react-test-renderer": "18.3.1",
    "babel-jest": "29.7.0",
    "babel-plugin-module-resolver": "5.0.2",
    "eslint": "8.57.1",
    "firebase-tools": "13.29.2",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=20"
  }
}
