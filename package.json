    "babel-jest": "29.7.0",
    "babel-plugin-module-resolver": "5.0.2",
    "eslint": "8.57.1",
    "firebase-tools": "13.29.2",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=20"
  }
}